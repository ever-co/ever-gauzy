{
	"name": "@gauzy/ui-config",
	"version": "0.1.0",
	"description": "A configuration library for the Gauzy framework, providing environment settings and configurations to be shared across various Angular applications within the Gauzy ecosystem.",
	"author": {
		"name": "Ever Co. LTD",
		"email": "ever@ever.co",
		"url": "https://ever.co"
	},
	"license": "AGPL-3.0",
	"private": true,
	"main": "dist/fesm2022/gauzy-ui-config.mjs",
	"module": "dist/fesm2022/gauzy-ui-config.mjs",
	"es2022": "dist/fesm2022/gauzy-ui-config.mjs",
	"esm2022": "dist/esm2022/gauzy-ui-config.mjs",
	"fesm2022": "dist/fesm2022/gauzy-ui-config.mjs",
	"typings": "dist/index.d.ts",
	"files": [
		"fesm2022",
		"esm2022",
		"lib",
		"dist"
	],
	"directories": {
		"lib": "dist"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"scripts": {
		"lib:build": "ng build ui-config --configuration development",
		"lib:build:prod": "yarn ts-node scripts/replace-env-files.ts && ng build ui-config --configuration production",
		"lib:watch": "ng build ui-config --watch --configuration development",
		"clean": "rimraf dist"
	},
	"peerDependencies": {
		"@angular/common": "^16.2.12",
		"@angular/core": "^16.2.12"
	},
	"dependencies": {
		"tslib": "^2.6.2"
	},
	"devDependencies": {
		"@types/node": "^20.14.9",
		"rimraf": "^3.0.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ever-co/ever-gauzy"
	},
	"bugs": {
		"url": "https://github.com/ever-co/ever-gauzy/issues"
	},
	"homepage": "https://ever.co",
	"keywords": [
		"gauzy",
		"angular",
		"configuration",
		"environment",
		"settings",
		"library"
	],
	"engines": {
		"node": ">=16.20.1",
		"yarn": ">=1.22.19"
	},
	"sideEffects": false
}
