{
	"name": "@gauzy/ui-config",
	"version": "0.0.1",
	"description": "A configuration library for the Gauzy framework, providing environment settings and configurations to be shared across various Angular applications within the Gauzy ecosystem.",
	"author": {
		"name": "Ever Co. LTD",
		"email": "ever@ever.co",
		"url": "https://ever.co"
	},
	"license": "AGPL-3.0",
	"files": [
		"dist"
	],
	"directories": {
		"lib": "dist"
	},
	"scripts": {
		"build": "ng-packagr -p ng-package.json --config tsconfig.lib.json",
		"build:prod": "ng-packagr -p ng-package.json --config tsconfig.lib.prod.json"
	},
	"resolutions": {
		"@types/mime": "3.0.4"
	},
	"devDependencies": {
		"@angular/cli": "^16.2.11",
		"@angular/compiler-cli": "^16.2.12",
		"@types/mime": "3.0.4",
		"@types/node": "^17.0.33",
		"ng-packagr": "16.2.3",
		"rimraf": "^3.0.2",
		"typescript": "5.1.6"
	},
	"peerDependencies": {
		"@angular/core": "^16.2.12",
		"zone.js": "^0.14.2"
	},
	"keywords": [
		"gauzy",
		"angular",
		"configuration",
		"environment",
		"settings",
		"library"
	],
	"sideEffects": false,
	"private": true
}
