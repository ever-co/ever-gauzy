{
	"name": "@gauzy/ui-config",
	"version": "0.1.0",
	"type": "commonjs",
	"description": "A configuration library for the Gauzy framework, providing environment settings and configurations to be shared across various Angular applications within the Gauzy ecosystem.",
	"author": {
		"name": "Ever Co. LTD",
		"email": "ever@ever.co",
		"url": "https://ever.co"
	},
	"license": "AGPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ever-co/ever-gauzy"
	},
	"bugs": {
		"url": "https://github.com/ever-co/ever-gauzy/issues"
	},
	"homepage": "https://ever.co",
	"keywords": [
		"gauzy",
		"angular",
		"configuration",
		"environment",
		"settings",
		"library"
	],
	"private": true,
	"main": "src/index.ts",
	"typings": "src/index.ts",
	"files": [
		"dist"
	],
	"directories": {
		"lib": "dist",
		"test": "__test__"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"scripts": {
		"lib:build": "nx build ui-config --configuration development",
		"lib:build:prod": "cross-env NODE_ENV=production yarn ts-node scripts/replace-env-files.ts --environment=prod && nx build ui-config --configuration production",
		"lib:watch": "nx build ui-config --watch --configuration development"
	},
	"peerDependencies": {
		"@angular/common": "^17.3.12",
		"@angular/core": "^17.3.12"
	},
	"dependencies": {
		"tslib": "^2.6.2",
		"zone.js": "^0.14.2"
	},
	"devDependencies": {
		"@types/jest": "^29.4.4",
		"@types/node": "^20.14.9",
		"cross-env": "^7.0.3",
		"jest-preset-angular": "^13.1.4"
	},
	"engines": {
		"node": ">=20.11.1",
		"yarn": ">=1.22.19"
	},
	"sideEffects": false
}
