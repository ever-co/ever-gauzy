{
	"lerna": "3.13.0",
	"npmClient": "yarn",
	"version": "0.1.0",
	"changelog": {
		"repo": "i4net/i4net",
		"cacheDir": ".changelog",
		"labels": {}
	},
	"packages": ["libs/*", "apps/*", "packages/*", "packages/plugins/*"],
	"useNx": true,
	"useWorkspaces": true,
	"command": {
		"exec": {
			"scope": ["@ngx-starter-kit/*"]
		},
		"clean": {
			"yes": true
		}
	},
	"npmClientArgs": ["--no-package-lock"]
}
